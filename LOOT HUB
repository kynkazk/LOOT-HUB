
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
-- Variáveis
local jogador = Players.LocalPlayer
local personagem = jogador.Character
local velocidadeBoost = 2
local saltoBoost = 2
-- Funções
local function aplicarVelocidadeBoost()
    personagem.Humanoid.WalkSpeed = personagem.Humanoid.WalkSpeed * velocidadeBoost
end
local function aplicarSaltoBoost()
    personagem.Humanoid.JumpPower = personagem.Humanoid.JumpPower * saltoBoost
end
-- Interface
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "Loot Hub"
ScreenGui.Parent = jogador.PlayerGui
local Frame = Instance.new("Frame")
Frame.Name = "Configurações"
Frame.Size = UDim2.new(0, 300, 0, 200)
Frame.Position = UDim2.new(0.5, -150, 0.5, -100)
Frame.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
Frame.BorderSizePixel = 0
local ImageLabel = Instance.new("ImageLabel")
ImageLabel.Name = "Logo"
